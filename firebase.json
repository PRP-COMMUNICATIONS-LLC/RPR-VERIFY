{
  "projects": {
    "default": "rpr-verify-b"
  },
  "hosting": {
    "site": "rpr-verify-b",
    "public": "frontend/dist/rpr-verify",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "cisReportApi",
        "region": "asia-southeast1"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "source": "backend/functions",
    "predeploy": [],
    "ingressSettings": "ALLOW_ALL",
    "invoker": "public"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}