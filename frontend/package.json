    "angular-eslint": "21.0.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.46.4",
    "zod": "^4.3.4"
  }
