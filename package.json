{
  "name": "rpr-client-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build": "ng build",
    "build:local": "ng build",
    "vercel-build": "ng build --configuration production && node scripts/prepare-vercel-output.js",
    "watch": "ng build --watch --configuration development",
    "test": "vitest run --passWithNoTests",
    "lint": "ng lint"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.8.2",
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/common": "^21.0.5",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.6",
    "@angular/router": "^21.0.0",
    "firebase": "^12.6.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.0",
    "angular-eslint": "21.0.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "fs-extra": "^11.3.2",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.4",
    "vitest": "^4.0.8"
  }
}
